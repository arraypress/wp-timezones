{
  "name": "arraypress/wp-timezones",
  "version": "1.0.0",
  "description": "A lightweight WordPress library for working with timezone data and utilities.",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-timezones",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\Timezones\\": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-timezones/issues",
    "source": "https://github.com/arraypress/wp-timezones"
  },
  "keywords": [
    "wordpress",
    "wp",
    "timezones",
    "timezone",
    "utc",
    "datetime",
    "regions",
    "arraypress"
  ]
}